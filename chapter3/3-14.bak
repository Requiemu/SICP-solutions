#lang planet neil/sicp

(define (mystery x)
  (define (loop x y)
    (if (null? x)
        y
        (let ((temp (cdr x)))
          (begin (set-cdr! x y)
                 (display x)
                 (display "<---")
                 (display y)
                 (newline)
                 (display temp)
                 (newline)
                 (loop temp x)))))
  (loop x '()))

(define v '(a b c d))

(define w (mystery v))

(display v)
(newline)

(display w)
(newline)
